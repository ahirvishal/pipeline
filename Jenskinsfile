
def remote = [:]
remote.name = "ec2"
remote.host = "ec2-13-233-39-165.ap-south-1.compute.amazonaws.com"
remote.allowAnyHosts = true

node 
{
    withCredentials([sshUserPrivateKey(credentialsId: 'ec2-instance', keyFileVariable: '/home/symphony/Downloads', usernameVariable: 'centos')]) {
        remote.user = centos
        remote.identityFile = '/home/symphony/Downloads/jen.pem'
        stage("SSH Steps Rocks!") 
        {
            echo $remote.name
            echo $remote.user
        }
        
    }
}
